    <groups>
        <group>
            <name>Templates RR</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>RR Huawei  SNMPv2 - Informacoes Extra</template>
            <name>RR Huawei  SNMPv2 - Informacoes Extra</name>
            <groups>
                <group>
                    <name>Templates RR</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>CPU</name>
                </application>
                <application>
                    <name>Interfaces</name>
                </application>
                <application>
                    <name>Memoria</name>
                </application>
                <application>
                    <name>Routes</name>
                </application>
                <application>
                    <name>Sistema</name>
                </application>
                <application>
                    <name>Uptime do Sistema</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Corrente Atual</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.2011.6.157.1.6.0</snmp_oid>
                    <key>hwCorrente</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <trends>90d</trends>
                    <units>w</units>
                    <applications>
                        <application>
                            <name>Sistema</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Device ESN</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.2011.5.25.188.1.1.0</snmp_oid>
                    <key>hwEsn</key>
                    <delay>1d</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Sistema</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Nome do Produto</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.2011.6.3.11.4.0</snmp_oid>
                    <key>hwNomeProduto</key>
                    <delay>1d</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Sistema</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Uptime do Sistema</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.1.3.0</snmp_oid>
                    <key>hwSysUpTime</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <units>uptime</units>
                    <applications>
                        <application>
                            <name>Sistema</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>0.01</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{change(0)}&lt;0</expression>
                            <name>Reiniciou</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Número de interfaces</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.2.1.0</snmp_oid>
                    <key>ifNumberInterfaces</key>
                    <delay>1h</delay>
                    <history>7d</history>
                    <description>The number of network interfaces (regardless of their current state) present on this system.</description>
                    <applications>
                        <application>
                            <name>Interfaces</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Versão do Sistema e Hardware</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.1.1.0</snmp_oid>
                    <key>sysDescr.0</key>
                    <delay>1d</delay>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <applications>
                        <application>
                            <name>Sistema</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{diff(0)}&gt;0</expression>
                            <name>Versão modificada</name>
                            <priority>WARNING</priority>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Discovery memoria</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.2011.6.3.5.1.1.2]</snmp_oid>
                    <key>hwMemoryDev</key>
                    <delay>10m</delay>
                    <lifetime>1d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Contagem de falhas de alocação de memória</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.5.1.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>hwMemoryDevFail[{#SNMPINDEX}]</key>
                            <delay>60</delay>
                            <applications>
                                <application>
                                    <name>Memoria</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Memoria Livre</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.5.1.1.3.{#SNMPINDEX}</snmp_oid>
                            <key>hwMemoryDevFree[{#SNMPINDEX}]</key>
                            <delay>60</delay>
                            <value_type>FLOAT</value_type>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memoria</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Memoria Utilizada</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.5.1.1.4.{#SNMPINDEX}</snmp_oid>
                            <key>hwMemoryDevRawSliceUsed[{#SNMPINDEX}]</key>
                            <delay>60</delay>
                            <value_type>FLOAT</value_type>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memoria</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Tamanho total da memória</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.5.1.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>hwMemoryDevSize[{#SNMPINDEX}]</key>
                            <delay>60</delay>
                            <value_type>FLOAT</value_type>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memoria</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Memoria Livre (%)</name>
                            <type>CALCULATED</type>
                            <key>memFreePercentage[{#SNMPINDEX}]</key>
                            <delay>60</delay>
                            <value_type>FLOAT</value_type>
                            <units>B</units>
                            <params>last(&quot;hwMemoryDevFree[{#SNMPINDEX}]&quot;)/last(&quot;hwMemoryDevSize[{#SNMPINDEX}]&quot;)*100</params>
                            <applications>
                                <application>
                                    <name>Memoria</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Memoria Utilizada (%)</name>
                            <type>CALCULATED</type>
                            <key>memUsedPercentage[{#SNMPINDEX}]</key>
                            <delay>60</delay>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <params>last(&quot;hwMemoryDevRawSliceUsed[{#SNMPINDEX}]&quot;)/last(&quot;hwMemoryDevSize[{#SNMPINDEX}]&quot;)*100</params>
                            <applications>
                                <application>
                                    <name>Memoria</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Memoria Livre</name>
                            <yaxismax>0</yaxismax>
                            <show_work_period>NO</show_work_period>
                            <show_triggers>NO</show_triggers>
                            <type>PIE</type>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>199C0D</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - Informacoes Extra</host>
                                        <key>memFreePercentage[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                        <graph_prototype>
                            <name>Memoria Utilizada</name>
                            <yaxismax>0</yaxismax>
                            <show_work_period>NO</show_work_period>
                            <show_triggers>NO</show_triggers>
                            <type>PIE</type>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>199C0D</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - Informacoes Extra</host>
                                        <key>memUsedPercentage[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovery interfaces</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.2.1.2.2.1.2, {#IFALIAS},.1.3.6.1.2.1.31.1.1.1.18]</snmp_oid>
                    <key>ifDescr</key>
                    <delay>10m</delay>
                    <lifetime>1d</lifetime>
                    <description>You may also consider using IF-MIB::ifType or IF-MIB::ifAlias for discovery depending on your filtering needs.&#13;
&#13;
{$SNMP_COMMUNITY} is a global macro.</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>Status de administrador da interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.2.2.1.7.{#SNMPINDEX}</snmp_oid>
                            <key>ifAdminStatus[{#SNMPVALUE}]</key>
                            <delay>5m</delay>
                            <history>1w</history>
                            <description>The desired state of the interface.</description>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>SNMP interface status (ifAdminStatus)</name>
                            </valuemap>
                        </item_prototype>
                        <item_prototype>
                            <name>Alias de interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.31.1.1.1.18.{#SNMPINDEX}</snmp_oid>
                            <key>ifAlias[{#SNMPVALUE}]</key>
                            <delay>1d</delay>
                            <history>1w</history>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Descrição da interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.2.2.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>ifDescr[{#SNMPVALUE}]</key>
                            <delay>1d</delay>
                            <history>1w</history>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <description>A textual string containing information about the interface.  This string should include the name of the manufacturer, the product name and the version of the interface hardware/software.</description>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Tráfego de entrada na interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.31.1.1.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>ifHCInOctets[{#SNMPVALUE}]</key>
                            <units>bps</units>
                            <description>The number of octets in valid MAC frames received on this interface, including the MAC header and FCS.</description>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Tráfego de saída na interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.31.1.1.1.10.{#SNMPINDEX}</snmp_oid>
                            <key>ifHCOutOctets[{#SNMPVALUE}]</key>
                            <units>bps</units>
                            <description>The number of octets transmitted in MAC frames on this interface, including the MAC header and FCS.</description>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Erros de entrada na interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.2.2.1.14.{#SNMPINDEX}</snmp_oid>
                            <key>ifInErrors[{#SNMPVALUE}]</key>
                            <delay>30m</delay>
                            <history>1w</history>
                            <description>For packet-oriented interfaces, the number of inbound packets that contained errors preventing them from being deliverable to a higher-layer protocol.  For character-oriented or fixed-length interfaces, the number of inbound transmission units that contained errors preventing them from being deliverable to a higher-layer protocol.</description>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Status operacional da interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.2.2.1.8.{#SNMPINDEX}</snmp_oid>
                            <key>ifOperStatus[{#SNMPVALUE}]</key>
                            <delay>5m</delay>
                            <history>1w</history>
                            <description>The current operational state of the interface.</description>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>SNMP interface status (ifOperStatus)</name>
                            </valuemap>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{diff(0)}=1</expression>
                                    <name>Status Operacional foi alterado no {HOST.NAME} interface {#SNMPVALUE}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Erros de saída na interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.2.2.1.20.{#SNMPINDEX}</snmp_oid>
                            <key>ifOutErrors[{#SNMPVALUE}]</key>
                            <delay>5m</delay>
                            <history>1w</history>
                            <description>For packet-oriented interfaces, the number of outbound packets that could not be transmitted because of errors. For character-oriented or fixed-length interfaces, the number of outbound transmission units that could not be transmitted because of errors.</description>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Erros na interface {#SNMPVALUE} ({#IFALIAS})</name>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>199C0D</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - Informacoes Extra</host>
                                        <key>ifInErrors[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <color>F63100</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - Informacoes Extra</host>
                                        <key>ifOutErrors[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                        <graph_prototype>
                            <name>Tráfego na interface {#SNMPVALUE} ({#IFALIAS})</name>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>00AA00</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - Informacoes Extra</host>
                                        <key>ifHCInOctets[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>3333FF</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - Informacoes Extra</host>
                                        <key>ifHCOutOctets[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
