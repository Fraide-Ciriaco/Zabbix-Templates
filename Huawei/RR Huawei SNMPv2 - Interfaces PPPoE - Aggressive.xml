                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.5.2.1.15.1.37.{#SNMPINDEX}</snmp_oid>
                            <key>login.down[{#USUARIO}]</key>
                            <history>30d</history>
