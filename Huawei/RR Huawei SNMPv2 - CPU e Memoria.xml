<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-08-26T20:08:55Z</date>
    <groups>
        <group>
            <name>Templates RR</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>RR Huawei  SNMPv2 - CPU e Memoria</template>
            <name>RR Huawei  SNMPv2 - CPU e Memoria</name>
            <groups>
                <group>
                    <name>Templates RR</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>CPU Load</name>
                </application>
                <application>
                    <name>Memória</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>CPU</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.2011.6.3.4.1.3]</snmp_oid>
                    <key>huaweihwCpuDevIndex</key>
                    <delay>1h</delay>
                    <lifetime>1d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Uso médio da CPU em 1 min slot {#SNMPINDEX}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.4.1.3.{#SNMPINDEX}</snmp_oid>
                            <key>huaweihwAvgDuty1min[{#SNMPINDEX}]</key>
                            <history>7d</history>
                            <trends>60d</trends>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>CPU Load</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Uso médio da CPU em 5 min slot {#SNMPINDEX}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.4.1.4.{#SNMPINDEX}</snmp_oid>
                            <key>huaweihwAvgDuty5min[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <history>7d</history>
                            <trends>60d</trends>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>CPU Load</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;80</expression>
                                    <name>CPU em {HOST.NAME} sobrecarregada</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Uso médio da CPU em 5 seg slot {#SNMPINDEX}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.4.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>huaweihwCpuDevDuty[{#SNMPINDEX}]</key>
                            <history>7d</history>
                            <trends>60d</trends>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>CPU Load</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>CPU slot {#SNMPINDEX}</name>
                            <ymin_type_1>FIXED</ymin_type_1>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>00C800</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - CPU e Memoria</host>
                                        <key>huaweihwCpuDevDuty[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <color>0000C8</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - CPU e Memoria</host>
                                        <key>huaweihwAvgDuty1min[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>3</sortorder>
                                    <color>C80000</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - CPU e Memoria</host>
                                        <key>huaweihwAvgDuty5min[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Memoria</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.2011.6.3.5.1.1.2]</snmp_oid>
                    <key>huaweihwMemoryDevModuleIndex</key>
                    <delay>1h</delay>
                    <lifetime>1d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Uso de memória em % slot {#SNMPINDEX}</name>
                            <type>CALCULATED</type>
                            <key>huaweiMemoryPFree[{#SNMPINDEX}]</key>
                            <delay>180</delay>
                            <units>%</units>
                            <params>100 * ( ( last(&quot;hwTotalMem[{#SNMPINDEX}]&quot;) - last(&quot;hwMemLivre[{#SNMPINDEX}]&quot;) ) / last(&quot;hwTotalMem[{#SNMPINDEX}]&quot;) )</params>
                            <applications>
                                <application>
                                    <name>Memória</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{avg(6m)}&gt;90</expression>
                                    <name>Low Memory on {HOST.NAME}</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Memória livre slot {#SNMPINDEX}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.5.1.1.3.{#SNMPINDEX}</snmp_oid>
                            <key>hwMemLivre[{#SNMPINDEX}]</key>
                            <delay>180</delay>
                            <history>7d</history>
                            <trends>60d</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memória</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Tamanho da memória slot {#SNMPINDEX}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.2011.6.3.5.1.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>hwTotalMem[{#SNMPINDEX}]</key>
                            <delay>180</delay>
                            <history>7d</history>
                            <trends>60d</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memória</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>HUAWEI MEMORY slot {#SNMPINDEX}</name>
                            <ymin_type_1>FIXED</ymin_type_1>
                            <ymax_type_1>ITEM</ymax_type_1>
                            <ymax_item_1>
                                <host>RR Huawei  SNMPv2 - CPU e Memoria</host>
                                <key>hwTotalMem[{#SNMPINDEX}]</key>
                            </ymax_item_1>
                            <graph_items>
                                <graph_item>
                                    <drawtype>DASHED_LINE</drawtype>
                                    <color>F63100</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - CPU e Memoria</host>
                                        <key>hwTotalMem[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>00C800</color>
                                    <item>
                                        <host>RR Huawei  SNMPv2 - CPU e Memoria</host>
                                        <key>hwMemLivre[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
