<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-11-03T17:46:57Z</date>
    <groups>
        <group>
            <name>Templates RR</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>RR Mikrotik SNMP Interf. Trafego PPP</template>
            <name>RR Mikrotik SNMP Interf. Trafego PPP</name>
            <description>By Rudimar Remontti&#13;
Trafego dos:&#13;
PPPOE, PPTP, SSTP, L2TP, OVPN</description>
            <groups>
                <group>
                    <name>Templates RR</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Interface Trafego</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Interfaces fisicas ppp</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#IFNAME},.1.3.6.1.2.1.2.2.1.2,{#IFALIAS},.1.3.6.1.2.1.31.1.1.1.18,{#IFTYPE},.1.3.6.1.2.1.2.2.1.3]</snmp_oid>
                    <key>mk.nome.interface.ppp</key>
                    <delay>10m</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#IFTYPE}</macro>
                                <value>23</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <description>Descobre nome das interfaces fisicas</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>In interface - $1 {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.31.1.1.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>mk.in.ppp[{#IFNAME}]</key>
                            <history>1w</history>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Interface Trafego</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                            <request_method>POST</request_method>
                        </item_prototype>
                        <item_prototype>
                            <name>Out interface  - $1 {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.31.1.1.1.10.{#SNMPINDEX}</snmp_oid>
                            <key>mk.out.ppp[{#IFNAME}]</key>
                            <history>1w</history>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Interface Trafego</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                            <request_method>POST</request_method>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Tr√°fego {#IFNAME} {#IFALIAS}</name>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>00C853</color>
                                    <item>
                                        <host>RR Mikrotik SNMP Interf. Trafego PPP</host>
                                        <key>mk.in.ppp[{#IFNAME}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>0091EA</color>
                                    <item>
                                        <host>RR Mikrotik SNMP Interf. Trafego PPP</host>
                                        <key>mk.out.ppp[{#IFNAME}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                    <request_method>POST</request_method>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
