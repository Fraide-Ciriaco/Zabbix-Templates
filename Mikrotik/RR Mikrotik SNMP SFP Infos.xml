<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-09-17T18:13:09Z</date>
    <groups>
        <group>
            <name>Templates RR</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>RR Mikrotik SNMP SFP Infos</template>
            <name>RR Mikrotik SNMP SFP Infos</name>
            <description>by Rudimar Remontti</description>
            <groups>
                <group>
                    <name>Templates RR</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Interfaces SFP</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Interfaces Descoberta</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#IFNAME},1.3.6.1.4.1.14988.1.1.19.1.1.2,{#IFALIAS},1.3.6.1.2.1.31.1.1.1.18]</snmp_oid>
                    <key>spf.if.discovery</key>
                    <delay>5m</delay>
                    <lifetime>1d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Bias Current: {#IFNAME} - {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.14988.1.1.19.1.1.8.{#SNMPINDEX}</snmp_oid>
                            <key>bias.current.[{#IFNAME}]</key>
                            <delay>5m</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <units>mA</units>
                            <applications>
                                <application>
                                    <name>Interfaces SFP</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>TX Power : {#IFNAME} - {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.14988.1.1.19.1.1.9.{#SNMPINDEX}</snmp_oid>
                            <key>interface.[tx.{#IFNAME}]</key>
                            <delay>5m</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <value_type>FLOAT</value_type>
                            <units>dBm</units>
                            <applications>
                                <application>
                                    <name>Interfaces SFP</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.001</params>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{prev()}&lt;-26</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{prev()}&gt;-26</recovery_expression>
                                    <name>Interface {#IFNAME} - {#IFALIAS} : Sinal TX Está acima de -26dBm</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Gbic Temperatura {#IFNAME} - {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.14988.1.1.19.1.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>sfp.temperatura.[{#IFNAME}]</key>
                            <delay>5m</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <units>C</units>
                            <applications>
                                <application>
                                    <name>Interfaces SFP</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;70</expression>
                                    <name>Temperatura  do SPF {#IFNAME} - {#IFALIAS}  acima de 70º</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;75</expression>
                                    <name>Temperatura  do SPF {#IFNAME} - {#IFALIAS}  acima de 75º</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Tensão : {#IFNAME} - {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.14988.1.1.19.1.1.7.{#SNMPINDEX}</snmp_oid>
                            <key>sfp.tensao.[{#IFNAME}]</key>
                            <delay>5m</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <units>V</units>
                            <applications>
                                <application>
                                    <name>Interfaces SFP</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>RX Power : {#IFNAME} - {#IFALIAS}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.14988.1.1.19.1.1.10.{#SNMPINDEX}</snmp_oid>
                            <key>sfp.[rx.{#IFNAME}]</key>
                            <delay>5m</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <value_type>FLOAT</value_type>
                            <units>dBm</units>
                            <applications>
                                <application>
                                    <name>Interfaces SFP</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.001</params>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{prev()}&lt;-27.</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{prev()}&gt;-27.</recovery_expression>
                                    <name>Interface {#IFNAME} - {#IFALIAS} : Sinal RX Está acima de -27dBm</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{RR Mikrotik SNMP SFP Infos:sfp.[rx.{#IFNAME}].change()}&gt;-2 or {RR Mikrotik SNMP SFP Infos:interface.[tx.{#IFNAME}].change()}&gt;-2</expression>
                            <name>Interface {#IFNAME} - {#IFALIAS} : Sinal teve alteração</name>
                            <status>DISABLED</status>
                            <discover>NO_DISCOVER</discover>
                            <priority>WARNING</priority>
                        </trigger_prototype>
                    </trigger_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Bias Current {#IFNAME} - {#IFALIAS}</name>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <drawtype>BOLD_LINE</drawtype>
                                    <color>FF8000</color>
                                    <yaxisside>RIGHT</yaxisside>
                                    <item>
                                        <host>RR Mikrotik SNMP SFP Infos</host>
                                        <key>bias.current.[{#IFNAME}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <drawtype>BOLD_LINE</drawtype>
                                    <color>FF8A65</color>
                                    <item>
                                        <host>RR Mikrotik SNMP SFP Infos</host>
                                        <key>sfp.tensao.[{#IFNAME}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                        <graph_prototype>
                            <name>Interface TX/RX : {#IFNAME} - {#IFALIAS}</name>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <drawtype>BOLD_LINE</drawtype>
                                    <color>00BFFF</color>
                                    <item>
                                        <host>RR Mikrotik SNMP SFP Infos</host>
                                        <key>sfp.[rx.{#IFNAME}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <drawtype>BOLD_LINE</drawtype>
                                    <color>EC407A</color>
                                    <item>
                                        <host>RR Mikrotik SNMP SFP Infos</host>
                                        <key>interface.[tx.{#IFNAME}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                        <graph_prototype>
                            <name>Temperatura {#IFNAME} - {#IFALIAS}</name>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>0080FF</color>
                                    <item>
                                        <host>RR Mikrotik SNMP SFP Infos</host>
                                        <key>sfp.temperatura.[{#IFNAME}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
