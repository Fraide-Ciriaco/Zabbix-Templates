<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-09-17T18:12:05Z</date>
    <groups>
        <group>
            <name>Templates RR</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>RR Mikrotik SNMP Interf. Comentadas Rate Advertise</template>
            <name>RR Mikrotik SNMP Interf. Comentadas Rate Advertise</name>
            <description>By Rudimar Remontti</description>
            <groups>
                <group>
                    <name>Templates RR</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Interfaces</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Interfaces comentada</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.2.1.31.1.1.1.18]</snmp_oid>
                    <key>mk.comentif</key>
                    <delay>5m</delay>
                    <lifetime>0</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Rate</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.2.2.1.5.{#SNMPINDEX}</snmp_oid>
                            <key>mk.coment.rate[{#SNMPVALUE}]</key>
                            <history>7d</history>
                            <trends>30d</trends>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Interfaces</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{diff(0)}=1</expression>
                                    <name>Advertise {#SNMPVALUE}</name>
                                    <priority>INFO</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
